<config
        xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
        xmlns='http://www.ehcache.org/v3'
        xsi:schemaLocation="
            http://www.ehcache.org/v3
            http://www.ehcache.org/schema/ehcache-core-3.7.xsd">

    <cache-template name="default">
        <expiry>
            <ttl unit="minutes">15</ttl>
        </expiry>

        <listeners>
            <listener>
                <class>de.drauschke.HomeNetworkService.speedtest.cache.CacheEventLogger</class>
                <event-firing-mode>ASYNCHRONOUS</event-firing-mode>
                <event-ordering-mode>UNORDERED</event-ordering-mode>
                <events-to-fire-on>CREATED</events-to-fire-on>
                <events-to-fire-on>EXPIRED</events-to-fire-on>
                <events-to-fire-on>EVICTED</events-to-fire-on>
            </listener>
        </listeners>

        <heap>1</heap>
    </cache-template>

    <cache alias="speedtestResult" uses-template="default">
        <key-type>org.springframework.cache.interceptor.SimpleKey</key-type>
        <value-type>de.drauschke.HomeNetworkService.model.SpeedtestResult</value-type>
    </cache>
</config>